<AnchorPane prefHeight="540.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.car.client.controllers.PersonalAccount">
   <children>
      <ImageView fitHeight="540.0" fitWidth="668.0" opacity="0.74">
         <image>
            <Image url="@../img/personalAccount.png" />
